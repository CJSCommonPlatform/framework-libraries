{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "id": "person",
  "properties": {
    "firstName": {
      "type": "string",
      "id": "firstName"
    },
    "lastName": {
      "type": "string",
      "id": "lastName"
    },
    "required": {
      "type": "boolean",
      "id": "required"
    },
    "signedInCount": {
      "type": "integer",
      "id": "signedInCount"
    },
    "ratio": {
      "type": "number",
      "id": "ratio"
    }
  },
  "additionalProperties": false,
  "required": [
    "firstName",
    "lastName",
    "required",
    "signedInCount",
    "ratio"
  ]
}
